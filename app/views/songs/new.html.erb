<h1>Create a Song</h1>

<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.text_field :note_contents, name: "song[note_contents][]", id: "song_notes_1" %>
  <%= f.text_field :note_contents, name: "song[note_contents][]", id: "song_notes_2" %>

  <%# <%= f.text_field :note_contents %>
  <%= f.text_field :genre_name, list: "genres_autocomplete" %>
  <datalist id="genres_autocomplete">
    <% Genre.all.each do |genre| %>
      <option value="<%= genre.name %>">
    <% end %>
  </datalist>
  <%= f.submit %>
<% end %>


